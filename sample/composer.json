{
    "type": "project",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "require": {
        "php": "^7.1.3",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "damax/common": "^1.2",
        "damax/enqueue-simple-bus-queue-interop": "^0.3.2",
        "doctrine/doctrine-fixtures-bundle": "^3.0",
        "doctrine/orm": "^2.6",
        "enqueue/dbal": "^0.8.32",
        "enqueue/enqueue-bundle": "^0.8.35",
        "long-running/long-running": "^0.2.0",
        "nelmio/api-doc-bundle": "^3.2",
        "pagerfanta/pagerfanta": "^2.0",
        "phpdocumentor/reflection-docblock": "^4.3",
        "ramsey/uuid-doctrine": "^1.5",
        "sensio/framework-extra-bundle": "^5.2",
        "simple-bus/asynchronous-bundle": "^3.0",
        "simple-bus/doctrine-orm-bridge": "^5.0",
        "simple-bus/symfony-bridge": "^5.1",
        "symfony/asset": "^4.1",
        "symfony/console": "^4.1",
        "symfony/flex": "^1.1",
        "symfony/framework-bundle": "^4.1",
        "symfony/property-access": "^4.1",
        "symfony/serializer": "^4.1",
        "symfony/twig-bundle": "^4.1",
        "symfony/validator": "^4.1",
        "symfony/web-link": "^4.1",
        "symfony/yaml": "^4.1"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^2.13",
        "symfony/dotenv": "^4.1",
        "symfony/web-server-bundle": "^4.1"
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "replace": {
        "paragonie/random_compat": "*",
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php71": "*",
        "symfony/polyfill-php70": "*",
        "symfony/polyfill-php56": "*"
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install --symlink --relative %PUBLIC_DIR%": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ]
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": true
        }
    }
}
